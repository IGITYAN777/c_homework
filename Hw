#include <stdio.h>

int main() {

  
  int x = 0;
  
  printf("Enter the  number: ");
  scanf("%d", &x);

  if(x % 1000000 == 0){
    x += 100000;
    printf("%d", x);
  }else{
    printf("%d", x);
  }
  
  return 0;
  
}


/////////////////////////////////////////////////////////////// Xndir 7   (New homework)


#include <stdio.h>

int main() {
  int x = 0;
  
  printf("Enter the number of elements: ");
  scanf("%d", &x);

  int arr[x];

  for (int i = 0; i < x; i++) {
    printf("Enter the number: ");
    scanf("%d", &arr[i]);
  }

  for (int j = 0; j < x; j++) {
    printf("%d ", arr[j]);
  }

  return 0;
}


/////////////////////////////////////////////////////////////// Xndir 8

#include <stdio.h>

int main() {
  int x = 0;
  int y = 0;

  printf("Enter the number of elements: ");
  scanf("%d", &x);

  char arr[x];

  for (int i = 0; i < x; i++) {
    printf("Enter the char: ");
    scanf(" %c", &arr[i]);
  }

  for (int j = 0; j < x; j++) {
    if ('0' <= arr[j] && arr[j] <= '9') {
      y = arr[j] - '0';
      printf("%d ", y);
    }
  }

  return 0;
}

/////////////////////////////////////////////////////////////// Xndir 9

#include <stdio.h>

int main() {
  
  int x = 0;
  int y = 0;

  printf("Enter the number of elements: ");
  scanf("%d", &x);

  char arr[x];

  for (int i = 0; i < x; i++) {
    printf("Enter the char: ");
    scanf(" %c", &arr[i]);
  }

  for (int j = (x - 1); j >= 0; j--) {
      printf(" %c", arr[j]);
  }

  return 0;
}

/////////////////////////////////////////////////////////////// Xndir 10

#include <stdio.h>

int main() {
  
  int x = 0;

  printf("Enter the length of square: ");
  scanf("%d", &x);

  for (int i = 0; i < x; i++) {
    printf("\n * ");
    for(int j = 0; j < (x - 1); j++){
      if(j == (x - 2)){
        printf("* ");
      }else if(i == 0 || i == x - 1){
        printf("* ");
      }else{
        printf("  ");
      }
    }
  }

  return 0;
}


/////////////////////////////////////////////////////////////// Xndir 11


#include <stdio.h>

int main()
{
    int input_val = 0;
    int index = 0;
    int num = 0;
    int x = 0;
    printf("Mutq qanaky: ");
    scanf("%d", &x);
    int arr[x];
    
    printf("Mutq tvery: ");
    
    for(int i = 0; i < x; i++){
        scanf("%d", &num);
        arr[i] = num;
    }
    
    printf("Mutq tivy: ");
    scanf("%d", &input_val);
    
     for(int i = 0; i < x; i++){
        if(arr[i] == input_val){
            index = i;
            break;
        }
    }   

    printf("%d", index);

    return 0;
}


/////////////////////////////////////////////////////////////// Xndir 12

#include <stdio.h>

int main()
{

    int num = 0;
    int x = 0;
    int only = 0;
    int p = 0;
    
    printf("Mutq qanaky: ");
    scanf("%d", &x);
    int arr[x];
    
    printf("Mutq tvery: ");
    
    for(int i = 0; i < x; i++){
        scanf("%d", &num);
        arr[i] = num;
    }
  
  
    for(int i = 0; i < x; i++){
        p = 0;
        for(int j = 0; j < x - i; j++){
            if(i != j){
                if(arr[i] == arr[j]){
                    p++;
                    break;
                }
            }
        } 
        if(p == 0){
            only = arr[i];
            break;
        }
    }  

    printf("%d", only);

    return 0;
}

/////////////////////////////////////////////////////////////// Xndir 13

#include <stdio.h>

int main()
{


    int length_1 = 0;
    int length_2 = 0;
    int num = 0;
    
    
    printf("Mutq qanaky: ");
    scanf("%d", &length_1);
    int arr1[length_1];
    
    
    printf("Mutq tvery arr1: ");
    for(int i = 0; i < length_1; i++){
        scanf("%d", &num);
        arr1[i] = num;
    }
    
    
    
    
    printf("Mutq qanaky: ");
    scanf("%d", &length_2);
    int arr2[length_2];
    
    printf("Mutq tvery arr2: ");
    for(int i = 0; i < length_2; i++){
        scanf("%d", &num);
        arr2[i] = num;
    }
    
    
    int arr[(length_1 + length_2)];
    
    
    for(int i = 0; i < (length_1 + length_2); i++){
        arr[i] = arr1[i];
    }

    for(int i = 0; i < (length_1 + length_2); i++){
        arr[(i + length_1)] = arr2[i];
    }

    for(int i = 0; i < (length_1 + length_2); i++){
        printf("%d", arr[i]);
    }

 
    return 0;
}

/////////////////////////////////////////////////////////////// Xndir 14

#include <stdio.h>

int main()
{
    
    int length = 0;
    printf("Mutq qanaky: ");
    scanf("%d", &length);
    char str[length];
    char a = 0;
    int b = 0;
    
    for(int i = 0; i < length; i++){
        printf("Mutq : ");
        scanf(" %c", &a);
        str[i] = a;
    }
    
    for(int i = 0; i < length/2; i++){
        if(str[i] == str[length - 1 - i]){
            continue;
        }else{
            b++;
        }
    }
    
    if(b == 0){
        printf("True");
    }else{
        printf("False");
    }

    return 0;
}


/////////////////////////////////////////////////////////////// Xndir 15

#include <stdio.h>

int main()
{
    int length = 0;
    int num = 0;
    int max = 0;
    int a = 0;
    
    printf("Mutqagrel qanaky: ");
    scanf("%d", &length);
    
    int arr[length];
    
    for(int i = 0; i < length; i++){
        printf("Mutq tiv: ");
        scanf("%d", &num);
        arr[i] = num;
    }
    
    for(int i = 0; i < length; i++){
        if(arr[i] > max){
            max = arr[i];
        }
    }    
    
    for(int i = 0; i < length; i++){
        if(arr[i] != max){
            if(arr[i] > a){
                a = arr[i];
            }
        }
    }      
    
    printf("%d", a);

    return 0;
}

/////////////////////////////////////////////////////////////// Xndir 16


#include <stdio.h>

int main()
{
    int matrix[][3] = { {1, 2, 3}, {4, 5, 6}, {7, 8, 9} };
    int sum = 0;
    
    for(int i=0; i<3; i++){
        for(int j=0; j<3; j++){
            if(i == j){
                sum += matrix[i][j];
            }
        }
    }
    
    printf("%d", sum);

    return 0;
}


/////////////////////////////////////////////////////////////// Xndir 17


#include <stdio.h>

int main()
{
    int matrix[][3] = { {1, 2, 3}, {4, 5, 6}, {7, 8, 9} };
    int sum = 0;
    
    
    for(int i=0; i<3; i++){
        for(int j=0; j<3; j++){
            if((i + j) == 2){
                sum += matrix[i][j];
            }
        }
    }
    
    printf("%d", sum);

    return 0;
}
