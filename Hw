#include <stdio.h>

int main() {

  //Xndir 2

  int x = 0;
  int y = 0;
  int z = 0;

  do{
    printf("Enter the first number: ");
    scanf("%d", &x);
    printf("Enter the secnd number: ");
    scanf("%d", &y);

    if(x > y){
      printf("The greatest number is %d \n" , x);
    }else if(x < y){
      printf("The greatest number is %d \n", y);
    }else if(x == y){
      printf("The numbers are equal \n");
    }else{
      printf("Enter numbers");
    }
  }while(z == 0);

  return 0;
}
