#include <stdio.h>

int main() {
  
  int x = 0;

  do{
    
  printf("Enter the year: ");
  scanf("%d", &x);
    
    if(x % 4 == 0){
      printf("Nahanj tari e \n");
    }else{
      printf("Nahanj tari che \n");
    }
  }while(x > 0);
  
  return 0;
  
}

/////////////////////////////////////////////////////////////// Xndir 5

#include <stdio.h>

int main() {

  int i = 1;
  int a = 0;
  
  int x = 0;
  int y = 0;
  int z = 0;

  do{
    printf("Enter the first number: ");
    scanf("%d", &x);
    printf("Enter the second number: ");
    scanf("%d", &y);
    printf("Enter the third number: ");
    scanf("%d", &z);

  if(x >= y && x >= z){
    while(i <= x){
      if(x % i == 0 && y % i == 0 && z % i == 0){
        a = i;
      }
      i++;
  }
    printf("%d", a);
    printf("\n");
  }else if(y >= x && y >= z){
    while(i <= y){
      if(x % i == 0 && y % i == 0 && z % i == 0){
        a = i;
      }
      i++;
  }
    printf("%d", a);
    printf("\n");
  }else if(z >= x && z >= y){
      while(i <= z){
      if(x % i == 0 && y % i == 0 && z % i == 0){
        a = i;
      }
      i++;
  }
    printf("%d", a);
    printf("\n");
  }
    
  }while(x >= 0 && y >= 0 && z >= 0);
  
  return 0;
  
}

