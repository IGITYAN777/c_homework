#include <stdio.h>

int main() {

  //Xndir 2

  int x = 0;
  int y = 0;
  
  printf("Enter the first number: ");
  scanf("%d", &x);
  printf("Enter the secnd number: ");
  scanf("%d", &y);
    
  if(x > y){
    printf("The greatest number is %d \n" , x);
  }else if(x < y){
    printf("The greatest number is %d ", y);
  }else if(x == y){
    printf("The numbers are equal");
  }else{
    printf("Enter numbers");
  }
  
  return 0;
}
